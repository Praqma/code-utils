<configuration>
    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%d{HH:mm:ss.SSS} %-5level %-50logger{0} - %msg%n</pattern>
        </encoder>
    </appender>

    <logger name="groovyx.net.http.ParserRegistry" level="error"/>
    <logger name="org.hibernate.validator.internal.util.Version" level="warn"/>

    <root level="info">
        <appender-ref ref="STDOUT"/>
    </root>
</configuration>
